<div class="container">
      <div class="row">
        <div class="filters">
          <div class="row">
            <div class=""></div>
          </div>
        </div>
        <div class="panel panel-success">
          <div class="panel-heading"><h4>Clicks by Offer</h4></div>
          <div class="panel-body">
            <table id="clicks_by_offers_tbl" class="table table-striped table-hover table-bordered">
              <thead class="">
              <tr>
                <th>Offer</th>
                <th>Clicks</th>
                <th>Reservations</th>
                <th>Checkouts</th>
                <th>Total Payout</th>
                <th>Affiliate Payout</th>
                <th>Better</th>
                <th>Cheaper</th>
                <th>Closer</th>
                <th>Create Date</th>
              </tr>
              </thead>
              <tbody>

              <%- @offers.each do |offer| %>
                  <tr>
                    <td>
                      <%= offer.name %>
                    </td>
                    <td>
                      <%= offer.clicks.count %>
                    </td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td>
                      <%= offer.created_at %>
                    </td>
                  </tr>

              <%- end %>

              </tbody>
            </table>
          </div>
          <div class="panel-footer">Last 30 days statistics</div>
        </div>

      </div>
